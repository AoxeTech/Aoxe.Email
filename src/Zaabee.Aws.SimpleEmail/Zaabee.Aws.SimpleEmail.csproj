<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;net6.0;net8.0;net9.0</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2024.1.10</PackageVersion>
        <Version>2024.1.10</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageProjectUrl>https://github.com/PicoHex</PackageProjectUrl>
        <RepositoryUrl>https://github.com/PicoHex/Zaabee.Email</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>Zaabee;Aws;SmtpClient;Email;Amazon.SimpleEmailV2</PackageTags>
        <Description>The email provider base by Amazon.SimpleEmailV2.</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
        <Authors>Roman Du</Authors>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="AWSSDK.SimpleEmailV2" Version="3.7.301.24" />
        <PackageReference Include="MimeKit" Version="4.5.0" />
        <PackageReference Include="Zaabee.Email.Abstractions" Version="2024.1.5" />
        <PackageReference Include="Zaabee.Extensions" Version="2024.1.5" />
    </ItemGroup>

</Project>
