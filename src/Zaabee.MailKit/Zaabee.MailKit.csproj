<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2021.1.1</PackageVersion>
        <Version>2021.1.1</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageProjectUrl>https://github.com/Mutuduxf/Zaabee.Email/tree/master/src/Zaabee.MailKit</PackageProjectUrl>
        <Description>The fluent style wrappers for MailKit.</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageTags>Zaabee MailKit Smtp Email</PackageTags>
        <LangVersion>Latest</LangVersion>
    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="MailKit" Version="2.11.1" />
      <PackageReference Include="Zaabee.Extensions" Version="2021.1.0" />
    </ItemGroup>
</Project>